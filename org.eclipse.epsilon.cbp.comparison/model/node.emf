@namespace(uri="node", prefix="")
package node;

class Node {
    attr String ID;
	attr Integer deep = 0;
	ref  Node parent;
	attr String name;
	attr String defName = "Foo";
	attr Integer[*] values;
	!unique attr Integer[*] listValues;
	ref Node refNode;
	val Node valNode;
	val Node[*] valNodes;
	ref Node[*] refNodes;
	!ordered ref Node[*] uorderedValNodes;  
}

