var n1 = new Node;
n1.name = "A";

var object = M.allContents.selectOne(node | node.name == "A");
object.println();



operation deleteObject(object){
	var i : Integer = object.valNodes.size()-1;
	while (i >= 0){
		var x = object.valNodes.get(i);
		deleteObject(x);
		i -= 1;
	}
	object.name.println();
	delete object;
}