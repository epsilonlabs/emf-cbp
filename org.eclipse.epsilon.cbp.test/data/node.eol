var root = new Node;
for(i in Sequence{0..1000}){
    var node = new Node;
	 node.name = i.asString();
	 root.valNodes.add(node);
}
deleteObject(root);
operation deleteObject(object){
	var i : Integer = object.valNodes.size()-1;
	while (i >= 0){
		var x = object.valNodes.get(i);
		deleteObject(x);
		i -= 1;
	}
	delete object;
}

