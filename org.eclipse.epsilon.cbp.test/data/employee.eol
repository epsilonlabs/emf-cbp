operation isCircular(targetObject, valueObject): Boolean {
	var result = false;	
	for (child in valueObject.manages){
		if (child == targetObject){
			result = true;
			break;
		}else{
			result = isCircular(
			targetObject, child);
		}
	}
	return result;
}