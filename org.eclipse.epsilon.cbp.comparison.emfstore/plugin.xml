<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>
   <extension
         id="example"
         point="org.eclipse.core.runtime.applications">
      <application
            cardinality="singleton-global"
            thread="main"
            visible="true">
         <run
               class="org.eclipse.epsilon.cbp.comparison.emfstore.test.Application">
         </run>
      </application>
   </extension>
   <extension
         point="org.eclipse.emf.emfstore.client.checksumErrorHandler">
      <checksumCheck
            errorHandler="org.eclipse.epsilon.cbp.comparison.emfstore.ESChecksumErrorHandler"
            isActive="false">
      </checksumCheck>
   </extension>
   <extension
         point="org.eclipse.emf.emfstore.client.changeRecordingOptions">
      <RecordingOptions
            useInMemoryChangePackage="true">
      </RecordingOptions>
   </extension>
</plugin>
